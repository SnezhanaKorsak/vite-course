{
  "name": "vite-course",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://SnezhanaKorsak.github.io/vite-course",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "staging": "vite --mode=staging",
    "format": "prettier --write \"./**/*.{ts,tsx,js,jsx,css,json}\" --no-error-on-unmatched-pattern",
    "lint:fix": "yarn lint --fix",
    "lint": "tsc --noEmit && eslint \"**/*.{js,jsx,ts,tsx}\" --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore",
    "test": "vitest",
    "deploy": "yarn build && gh-pages -d dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/vite-plugin-react-svg": "^0.2.5",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^6.1.1",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "sharp": "^0.33.3",
    "svgo": "^3.2.0",
    "typescript": "^5.4.3",
    "vite": "^5.2.0",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-image-optimizer": "^1.1.7",
    "vite-plugin-inspect": "^0.8.3",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.4.0"
  }
}
